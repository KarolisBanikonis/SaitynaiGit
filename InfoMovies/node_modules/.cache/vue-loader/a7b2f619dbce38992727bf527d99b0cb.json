e'\r\nimport axios from 'axios'\r\nexport default{\r\n    name:'UpdateCompany',\r\n    data()\r\n    {\r\n        return {\r\n            name:'',\r\n            description:'',\r\n            token:''\r\n        }\r\n    },\r\n    methods:{\r\n        async updateCompany()\r\n        {\r\n            this.token=localStorage.getItem(\"token\");\r\n            let id = localStorage.getItem(\"chosenCompany\");\r\n            let request = await axios.put(\"https://infomoviesapi.azurewebsites.net/api/companies/\" + id,{\r\n                name:this.name,\r\n                description:this.description\r\n            }, { headers: {\"Authorization\" : `Bearer ${this.token}`} });\r\n            if (request.status == 200){\r\n                this.$router.push({name:'Companies'})\r\n            }\r\n        }\r\n    },\r\n    async mounted(){\r\n        let role = localStorage.getItem(\"userRole\");\r\n        if(role != \"Admin\"){\r\n            this.$router.push({name: 'Companies'});\r\n        }\r\n    },\r\n    components:{\r\n        Header\r\n    }\r\n}\r\n</script>\r\n\r\n<style>\r\n.Logo{\r\n    width: 75px\r\n}\r\n.register input{\r\n    width: 300px;\r\n    height: 40px;\r\n    padding-left: 20px;\r\n    display: block;\r\n    margin-bottom: 30px;\r\n    margin-right: auto;\r\n    margin-left: auto;\r\n    border: 2px solid skyblue;\r\n}\r\n.register button{\r\n    width: 150px;\r\n    height: 50px;\r\n    transition-duration: 0.4s;\r\n}\r\n\r\n.login button:hover {\r\n  background-color: skyblue; \r\n  color: white;\r\n}\r\n</style>"]}]}