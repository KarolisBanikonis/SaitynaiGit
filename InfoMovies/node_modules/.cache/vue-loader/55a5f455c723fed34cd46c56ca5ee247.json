nts/Companies.vue","sourceRoot":"","sourcesContent":["<template>\r\n<Header />\r\n    <h1>Filmus kuriančių kompanijų sąrašas</h1>\r\n    <button v-if=\"showIfAdmin()\" v-on:click=\"createCompany()\">Sukurkite kompaniją</button>\r\n    <br>\r\n    <br>\r\n    <div id=\"app\">\r\n    <button\r\n      type=\"button\"\r\n      class=\"btn\"\r\n      @click=\"showModal\"\r\n    >\r\n      Open Modal!\r\n    </button>\r\n\r\n    <ModalCompany v-show=\"isModalVisible\" @close=\"closeModal\">\r\n        <template v-slot:body>\r\n            <h2>Kompanija</h2>\r\n            <li>Id - {{this.company.id}}</li>\r\n            <li>Pavadinimas - {{this.company.name}}</li>\r\n            <li>Aprašymas - {{this.company.description}}</li>\r\n        </template>\r\n    </ModalCompany>\r\n  </div>\r\n    <table>\r\n        <td>Pavadinimas</td>\r\n        <td>Aprašymas</td>\r\n        <td></td>\r\n        <td v-if=\"showIfAdmin()\"></td>\r\n        <td v-if=\"showIfAdmin()\"></td>\r\n        <tr v-for=\"company in companies\" :key=\"company.id\">\r\n            <td v-on:click=\"getMovie(company)\">{{company.name}}</td>\r\n            <td v-on:click=\"getMovie(company)\">{{company.description}}</td>\r\n            <td v-on:click=\"getMovies(company.id)\">Filmai</td>\r\n            <td v-if=\"showIfAdmin()\" v-on:click=\"updateCompany(company.id)\">Atnaujinti</td>\r\n            <td v-if=\"showIfAdmin()\" v-on:click=\"deleteCompany(company.id)\">Ištrinti</td>\r\n        </tr>   \r\n    </table>\r\n</template>\r\n<script>\r\n\r\nimport axios from 'axios'\r\nimport Header from './Header.vue'\r\nimport ModalCompany from \"./ModalCompany\";\r\nexport default{\r\n    name:'Companies',\r\n    data(){\r\n        return {\r\n            companies:[],\r\n            token:'',\r\n            isModalVisible: false,\r\n            company:[]\r\n        }\r\n    },\r\n    components:{\r\n        Header,\r\n        ModalCompany\r\n    },\r\n    methods:{\r\n        showModal() {\r\n            this.isModalVisible = true;\r\n        },\r\n        closeModal() {\r\n            this.isModalVisible = false;\r\n        },\r\n        showIfAdmin(){\r\n            let role = localStorage.getItem(\"userRole\");\r\n            if(role == \"Admin\"){\r\n                return true;\r\n            }\r\n            return false;\r\n        },\r\n        createCompany(){\r\n            this.$router.push({name: 'CreateCompany'});\r\n        },\r\n        updateCompany(id){\r\n            localStorage.setItem(\"chosenCompany\", id);\r\n            this.$router.push({name: 'UpdateCompany'});\r\n        },\r\n        async deleteCompany(id){\r\n            await axios.delete(\"https://infomoviesapi.azurewebsites.net/api/companies/\"+id, { headers: {\"Authorization\" : `Bearer ${this.token}`} });\r\n            location.reload();\r\n        },\r\n        async getMovie(company){\r\n            /*let request = await axios.get(\"https://infomoviesapi.azurewebsites.net/api/companies/\"+id);\r\n            this.company = JSON.stringify(request.data);\r\n            localStorage.setItem(\"chosenCompany\", this.company);*/\r\n            this.company = company;\r\n            this.isModalVisible = true;\r\n        },\r\n        async getMovies(id){\r\n            let request =await axios.get(\"https://infomoviesapi.azurewebsites.net/api/companies/\"+id);\r\n            localStorage.setItem(\"chosenCompany\", JSON.stringify(request.data));\r\n            this.$router.push({name: 'Movies'}); \r\n        }\r\n    },\r\n    async mounted(){\r\n        this.token = localStorage.getItem(\"token\");\r\n        let request = await axios.get(\"https://infomoviesapi.azurewebsites.net/api/companies/\");\r\n        this.companies = request.data;\r\n    }\r\n}\r\n</script>\r\n"]}]}