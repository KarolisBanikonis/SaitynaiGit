AAC,CAAC,CAAC,CAAC,CAAC,EAAE,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,EAAE,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,EAAE,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,EAAE,CAAC;QAC7E,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,EAAE,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,EAAE,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC;QAC/C,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,EAAE,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC;;IAEvE,CAAC,CAAC,CAAC,CAAC,CAAC","file":"C:/Users/Karolis/Desktop/infomovies/src/components/Login.vue","sourceRoot":"","sourcesContent":["<template>\r\n    <img class=\"signInLogo\" src=\"../assets/logo.png\">\r\n    <h1>Prisijungimas</h1>\r\n    <div class=\"login\">\r\n        <input type=\"text\" v-model=\"userName\" placeholder=\"Įveskite vartotojo vardą\" />\r\n        <input type=\"password\" v-model=\"password\" placeholder=\"Įveskite slaptažodį\" />\r\n        <button v-on:click=\"login\" >Prisijungti</button>\r\n        <button @click=\"$router.push({name:'SignUp'})\">Registruotis</button>\r\n\r\n    </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from 'axios'\r\nimport jwt_decode from \"jwt-decode\"\r\nexport default {\r\n    name:'Login',\r\n    data()\r\n    {\r\n        return {\r\n            userName:'',\r\n            password:''\r\n        }\r\n    },\r\n    methods:{\r\n        async login()\r\n        {\r\n            let request = await axios.post(\"https://infomoviesapi.azurewebsites.net/api/login\",{\r\n                userName:this.userName,\r\n                password:this.password\r\n            });\r\n            if (request.status == 200){\r\n                let jwtToken = JSON.stringify(request.data)\r\n                let accessToken = JSON.parse(jwtToken).accessToken;\r\n                localStorage.setItem(\"token\", accessToken)\r\n                var decodedToken = jwt_decode(accessToken);\r\n                if(JSON.stringify(decodedToken).includes(\"SimpleUser\")){\r\n                    localStorage.setItem(\"userRole\", \"SimpleUser\")\r\n                }\r\n                if(JSON.stringify(decodedToken).includes(\"Admin\")){\r\n                    localStorage.setItem(\"userRole\", \"Admin\")\r\n                }\r\n                this.$router.push({name:'Companies'})\r\n            }\r\n        }\r\n    },\r\n    mounted()\r\n    {\r\n        let user= localStorage.getItem(\"token\");\r\n        if (user){\r\n            this.$router.push({name:'Companies'})\r\n        }\r\n    }\r\n}\r\n</script>\r\n"]}]}