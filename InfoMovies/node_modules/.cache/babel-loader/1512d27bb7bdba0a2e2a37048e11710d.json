AAAD;AAAX,iBAJiB,CAJxB;;AAAA;AAIQ,gBAAA,OAJR;;AASI,oBAAI,OAAO,CAAC,MAAR,IAAkB,GAAtB,EAA0B;AACtB,kBAAA,KAAI,CAAC,OAAL,CAAa,IAAb,CAAkB;AAAC,oBAAA,IAAI,EAAC;AAAN,mBAAlB;AACJ;;AAXJ;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAYA;AAdI,GAXE;AA2BJ,EAAA,OA3BI,qBA2BK;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AACP,cAAA,IADO,GACA,YAAY,CAAC,OAAb,CAAqB,UAArB,CADA;;AAEX,kBAAG,IAAG,IAAK,OAAX,EAAmB;AACf,gBAAA,MAAI,CAAC,OAAL,CAAa,IAAb,CAAkB;AAAC,kBAAA,IAAI,EAAE;AAAP,iBAAlB;AACJ;;AAJW;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAKd,GAhCS;AAiCV,EAAA,UAAU,EAAC;AACP,IAAA,MAAK,EAAL;AADO;AAjCD,CAAd","sourcesContent":["<template>\r\n<Header />\r\n    <img class=\"Logo\" src=\"../assets/logo.png\">\r\n    <h1>Sukurkite filmą</h1>\r\n    <br>\r\n    <br>\r\n    <button @click=\"$router.push('Movies')\">Grįžti į filmų sąrašą</button>\r\n    <br>\r\n    <br>\r\n    <div class=\"register\">\r\n        <input type=\"text\" v-model=\"title\" placeholder=\"Įveskite filmo pavadinimą\" />\r\n        <input type=\"text\" v-model=\"story\" placeholder=\"Įveskite filmo aprašymą\" />\r\n        <input type=\"text\" v-model=\"score\" placeholder=\"Įveskite filmo įvertinimą\" />\r\n        <button v-on:click=\"createMovie()\" >Sukurti</button>\r\n    </div>\r\n</template>\r\n<script>\r\nimport Header from './Header.vue'\r\nimport axios from 'axios'\r\nexport default{\r\n    name:'CreateMovie',\r\n    data()\r\n    {\r\n        return {\r\n            title:'',\r\n            story:'',\r\n            score:'',\r\n            token:''\r\n        }\r\n    },\r\n    methods:{\r\n        async createMovie()\r\n        {\r\n            this.token=localStorage.getItem(\"token\");\r\n            let chosenCompany = localStorage.getItem(\"chosenCompany\");\r\n            let companyId = JSON.parse(chosenCompany).id;\r\n            let request = await axios.post(\"https://infomoviesapi.azurewebsites.net/api/companies/\" + companyId + \"/movies/\",{\r\n                title:this.title,\r\n                story:this.story,\r\n                score:this.score\r\n            }, { headers: {\"Authorization\" : `Bearer ${this.token}`} });\r\n            if (request.status == 201){\r\n                this.$router.push({name:'Movies'})\r\n            }\r\n        }\r\n    },\r\n    async mounted(){\r\n        let role = localStorage.getItem(\"userRole\");\r\n        if(role != \"Admin\"){\r\n            this.$router.push({name: 'Companies'});\r\n        }\r\n    },\r\n    components:{\r\n        Header\r\n    }\r\n}\r\n</script>\r\n\r\n<style>\r\n.Logo{\r\n    width: 75px\r\n}\r\n.register input{\r\n    width: 300px;\r\n    height: 40px;\r\n    padding-left: 20px;\r\n    display: block;\r\n    margin-bottom: 30px;\r\n    margin-right: auto;\r\n    margin-left: auto;\r\n    border: 2px solid skyblue;\r\n}\r\n.register button{\r\n    width: 150px;\r\n    height: 50px;\r\n    transition-duration: 0.4s;\r\n}\r\n\r\n.login button:hover {\r\n  background-color: skyblue; \r\n  color: white;\r\n}\r\n</style>"],"sourceRoot":""}]}