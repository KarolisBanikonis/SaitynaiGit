n                this.$router.push({name:'Comments'})\r\n            }\r\n        }\r\n    },\r\n    async mounted(){\r\n        let role = localStorage.getItem(\"userRole\");\r\n        if(role != \"Admin\"){\r\n            this.$router.push({name: 'Companies'});\r\n        }\r\n    },\r\n    components:{\r\n        Header\r\n    }\r\n}\r\n</script>"],"sourceRoot":""}]}