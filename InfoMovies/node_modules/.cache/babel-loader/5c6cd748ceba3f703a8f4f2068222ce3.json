A0B,EAZ9C;AAaQ,gBAAA,EAbR,GAaa,YAAY,CAAC,OAAb,CAAqB,aAArB,CAbb;AAAA;AAAA,uBAcwB,KAAK,CAAC,GAAN,CAAU,2DAA2D,SAA3D,GAAuE,UAAvE,GAAoF,EAA9F,EAAiG;AACjH,kBAAA,KAAK,EAAC,KAAI,CAAC,KADsG;AAEjH,kBAAA,KAAK,EAAC,KAAI,CAAC,KAFsG;AAGjH,kBAAA,KAAK,EAAC,KAAI,CAAC;AAHsG,iBAAjG,EAIjB;AAAE,kBAAA,OAAO,EAAE;AAAC,sDAA4B,KAAI,CAAC,KAAjC;AAAD;AAAX,iBAJiB,EAIwC,KAJxC,CAI8C,UAAA,KAAI,EAAK;AACnE,kBAAA,OAAO,CAAC,GAAR,CAAY,KAAZ;AACH,iBANe,CAdxB;;AAAA;AAcQ,gBAAA,OAdR;;AAqBI,oBAAI,OAAO,CAAC,MAAR,IAAkB,GAAtB,EAA0B;AACtB,kBAAA,KAAI,CAAC,OAAL,CAAa,IAAb,CAAkB;AAAC,oBAAA,IAAI,EAAC;AAAN,mBAAlB;AACJ;;AAvBJ;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAwBA;AA1BI,GAXE;AAuCV,EAAA,OAvCU,qBAuCD;AACL,QAAI,IAAG,GAAI,YAAY,CAAC,OAAb,CAAqB,UAArB,CAAX;;AACA,QAAG,IAAG,IAAK,OAAX,EAAmB;AACf,WAAK,OAAL,CAAa,IAAb,CAAkB;AAAC,QAAA,IAAI,EAAE;AAAP,OAAlB;AACJ;AACH,GA5CS;AA6CV,EAAA,UAAU,EAAC;AACP,IAAA,MAAK,EAAL;AADO;AA7CD,CAAd","sourcesContent":["<template>\r\n<Header />\r\n    <img class=\"Logo\" src=\"../assets/logo.png\">\r\n    <h1>Atnaujinkite duomenis apie filmą</h1>\r\n    <br>\r\n    <br>\r\n    <div class=\"register\">\r\n        <button @click=\"$router.push('Movies')\">Grįžti į filmų sąrašą</button>\r\n    </div>\r\n    <br>\r\n    <br>\r\n    <div class=\"register\">\r\n        <input type=\"text\" v-model=\"title\" placeholder=\"Įveskite filmo pavadinimą\" />\r\n        <input type=\"text\" v-model=\"story\" placeholder=\"Įveskite filmo aprašymą\" />\r\n        <input type=\"text\" v-model=\"score\" placeholder=\"Įveskite filmo įvertinimą\" />\r\n        <button v-on:click=\"updateMovie()\" >Atnaujinti</button>\r\n    </div>\r\n    <p class=\"footer\">InfoMovies</p>\r\n</template>\r\n<script>\r\nimport Header from './Header.vue'\r\nimport axios from 'axios'\r\nexport default{\r\n    name:'UpdateMovie',\r\n    data()\r\n    {\r\n        return {\r\n            title:'',\r\n            story:'',\r\n            score:'',\r\n            token:''\r\n        }\r\n    },\r\n    methods:{\r\n        async updateMovie()\r\n        {\r\n            if(this.title == ''){\r\n                alert(\"Įveskite filmo pavadinimą\");\r\n            }\r\n            else if(this.story == ''){\r\n                alert(\"Įveskite filmo aprašymą\");\r\n            }\r\n            else if(this.score == ''){\r\n                alert(\"Įveskite filmo įvertinimą\");\r\n            }\r\n            this.token=localStorage.getItem(\"token\");\r\n            let chosenCompany = localStorage.getItem(\"chosenCompany\");\r\n            let companyId = JSON.parse(chosenCompany).id;\r\n            let id = localStorage.getItem(\"chosenMovie\");\r\n            let request = await axios.put(\"https://infomoviesapi.azurewebsites.net/api/companies/\" + companyId + \"/movies/\" + id,{\r\n                title:this.title,\r\n                story:this.story,\r\n                score:this.score\r\n            }, { headers: {\"Authorization\" : `Bearer ${this.token}`} }).catch(error => {\r\n                    console.log(error);\r\n                });\r\n            if (request.status == 200){\r\n                this.$router.push({name:'Movies'})\r\n            }\r\n        }\r\n    },\r\n    mounted(){\r\n        let role = localStorage.getItem(\"userRole\");\r\n        if(role != \"Admin\"){\r\n            this.$router.push({name: 'Companies'});\r\n        }\r\n    },\r\n    components:{\r\n        Header\r\n    }\r\n}\r\n</script>"],"sourceRoot":""}]}