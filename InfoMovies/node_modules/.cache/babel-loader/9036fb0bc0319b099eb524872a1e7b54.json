,KAAI,CAAC,QADiE;AAE/E,kBAAA,QAAQ,EAAC,KAAI,CAAC;AAFiE,iBAA/D,EAGjB,KAHiB,CAGX,UAAA,KAAI,EAAK;AACd,kBAAA,OAAO,CAAC,GAAR,CAAY,KAAZ;AACA,kBAAA,KAAI,CAAC,cAAL,GAAsB,IAAtB;AACH,iBANmB,CAR5B;;AAAA;AAQY,gBAAA,OARZ;;AAeQ,oBAAI,OAAO,CAAC,MAAR,IAAkB,GAAtB,EAA0B;AAClB,kBAAA,QADkB,GACP,IAAI,CAAC,SAAL,CAAe,OAAO,CAAC,IAAvB,CADO;AAElB,kBAAA,WAFkB,GAEJ,IAAI,CAAC,KAAL,CAAW,QAAX,EAAqB,WAFjB;AAGtB,kBAAA,YAAY,CAAC,OAAb,CAAqB,OAArB,EAA8B,WAA9B;AACI,kBAAA,YAJkB,GAIH,UAAU,CAAC,WAAD,CAJP;;AAKtB,sBAAG,IAAI,CAAC,SAAL,CAAe,YAAf,EAA6B,QAA7B,CAAsC,YAAtC,CAAH,EAAuD;AACnD,oBAAA,YAAY,CAAC,OAAb,CAAqB,UAArB,EAAiC,YAAjC;AACJ;;AACA,sBAAG,IAAI,CAAC,SAAL,CAAe,YAAf,EAA6B,QAA7B,CAAsC,OAAtC,CAAH,EAAkD;AAC9C,oBAAA,YAAY,CAAC,OAAb,CAAqB,UAArB,EAAiC,OAAjC;AACJ;;AACA,kBAAA,KAAI,CAAC,OAAL,CAAa,IAAb,CAAkB;AAAC,oBAAA,IAAI,EAAC;AAAN,mBAAlB;AACJ;;AA3BR;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AA6BA;AAlCI,GAdG;AAkDX,EAAA,OAlDW,qBAmDX;AACI,QAAI,IAAI,GAAE,YAAY,CAAC,OAAb,CAAqB,OAArB,CAAV;;AACA,QAAI,IAAJ,EAAS;AACL,WAAK,OAAL,CAAa,IAAb,CAAkB;AAAC,QAAA,IAAI,EAAC;AAAN,OAAlB;AACJ;AACJ;AAxDW,CAAf","sourcesContent":["<template>\r\n<Header />\r\n    <ModalWindow v-show=\"isModalVisible\" @close=\"closeModal\">\r\n        <template v-slot:body>\r\n            <h2>Neteisingi duomenys</h2>\r\n        </template>\r\n    </ModalWindow>\r\n    \r\n    <h1>Prisijungimas</h1>\r\n    <div class=\"register\">\r\n        <input type=\"text\" v-model=\"userName\" placeholder=\"Įveskite vartotojo vardą\" />\r\n        <input type=\"password\" v-model=\"password\" placeholder=\"Įveskite slaptažodį\" />\r\n        <button v-on:click=\"login\" >Prisijungti</button>\r\n    </div>\r\n    <p class=\"footer\">InfoMovies</p>\r\n</template>\r\n\r\n<script>\r\nimport ModalWindow from \"./ModalWindow\";\r\nimport axios from 'axios'\r\nimport jwt_decode from \"jwt-decode\"\r\nimport Header from './Header.vue'\r\nexport default {\r\n    name:'Login',\r\n    data()\r\n    {\r\n        return {\r\n            userName:'',\r\n            password:'',\r\n            isModalVisible: false,\r\n        }\r\n    },\r\n    components:{\r\n        Header,\r\n        ModalWindow\r\n    },\r\n    methods:{\r\n        closeModal() {\r\n            this.isModalVisible = false;\r\n        },\r\n        async login()\r\n        {\r\n            if(this.userName == ''){\r\n                alert(\"Įveskite vartotojo vardą\");\r\n            }\r\n            else if(this.password == ''){\r\n                alert(\"Įveskite slaptažodį\");\r\n            }\r\n            else{\r\n                let request = await axios.post(\"https://infomoviesapi.azurewebsites.net/api/login\",{\r\n                    userName:this.userName,\r\n                    password:this.password\r\n                }).catch(error => {\r\n                    console.log(error);\r\n                    this.isModalVisible = true;\r\n                });\r\n                if (request.status == 200){\r\n                    let jwtToken = JSON.stringify(request.data)\r\n                    let accessToken = JSON.parse(jwtToken).accessToken;\r\n                    localStorage.setItem(\"token\", accessToken)\r\n                    var decodedToken = jwt_decode(accessToken);\r\n                    if(JSON.stringify(decodedToken).includes(\"SimpleUser\")){\r\n                        localStorage.setItem(\"userRole\", \"SimpleUser\")\r\n                    }\r\n                    if(JSON.stringify(decodedToken).includes(\"Admin\")){\r\n                        localStorage.setItem(\"userRole\", \"Admin\")\r\n                    }\r\n                    this.$router.push({name:'Home'})\r\n                }\r\n            }\r\n        }\r\n    },\r\n    mounted()\r\n    {\r\n        let user= localStorage.getItem(\"token\");\r\n        if (user){\r\n            this.$router.push({name:'Home'})\r\n        }\r\n    }\r\n}\r\n</script>\r\n"],"sourceRoot":""}]}